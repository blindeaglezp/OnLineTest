*{margin:0px;padding:0px;}
body{background:#262626;font-family:"微软雅黑";font-size: 14px;color:#666;}
.container{width:1200px;height:768px;margin:0 auto;background: cornflowerblue;}

/*start exam1*/
.empty1{width:20%;height:100%;float:left;}
.exam1_exam_rule{width:60%;height:80%;float:left;border-radius:10px;background:aliceblue;margin:30px auto;}
.exam1_exam_rule_content{width:90%;height:80%;border:1px groove darkgray;margin:40px auto 10px auto;}
.exam1_exam_rule_content .p1{padding:20px 20px 10px 20px;}
.exam1_exam_rule_content .p2{padding:0px 20px;text-indent:28px;}
.empty2{width:100%;height:10px;}
.exam1_exam_rule_button{width:100%;height:10%;text-align:center;}
.exam1_exam_rule_button a{text-decoration:none;padding:5px 10px;border-radius:5px;color:black;border:2px groove black;}
.exam1_exam_rule_button a:hover{background-color:darkgray;}
/*exam1 end*/

/*start exam2*/
.empty3{width:100%;height:10%;}
.exam2_content{width:60%;height:80%;background:aqua;border:3px groove darkgray;border-radius:20px;margin:0 auto;}
.exam2_stu_info{width:80%;height:10%;margin:30px auto;border-radius:50%;background:aliceblue;}
.exam2_stu_info ul{list-style:none;}
.exam2_stu_info ul li{float:left;margin:18px 20px;}
.exam2_testpaper_select{width:80%;height:10%;margin:30px auto;border-radius:50%;background:aliceblue;}
.exam2_testpaper_select ul{list-style:none;}
.exam2_testpaper_select ul li{float:left;margin:18px 80px 18px 30px;}
.exam2_confirm_testpaper{text-align:center;}
.exam2_confirm_testpaper a{width:100px;height:30px;text-decoration:none;color: darkmagenta;font-size:18px;border:2px groove darkgray;
    border-radius:5px;background-color:antiquewhite;padding:5px 10px;}
.exam2_confirm_testpaper a:hover{background:darkgray;}
.exam2_testpaper_info{width:400px;height:150px;margin:50px auto;background:azure;}
.exam2_testpaper_info ul{list-style:none;padding:25px 40px;}
.exam2_testpaper_info ul li{list-style:none;line-height:200%;}
.exam2_start_test{width:100%;height:50px;}
.exam2_start_test a{width:100px;height:30px;padding:5px 10px;border-radius:8px;background:antiquewhite;border:2px groove darkgray;
    text-decoration:none;font-size:24px;margin-left:70%;display:none;}
.exam2_start_test a:hover{background-color:darkgray;}
/*exam2 end*/